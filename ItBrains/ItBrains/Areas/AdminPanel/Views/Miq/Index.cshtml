
@model List<Miq>

<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div class="d-flex justify-content-between mt-2">
                    <h3 class="card-title pt-3">Miq </h3>
                    <div class="button-effect">
                        <a class="effect effect-2 create" asp-action="Create"> Yenisi yarat <i class="mdi mdi-arrow-right-bold"></i></a>
                    </div>
                </div>

                <div class="table-responsive">
                    <table class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                
                                <th>
                                    Link
                                </th>
                                <th>
                                    Əməliyatlar
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (Miq item in Model)
                            {
                            <tr>
                                
                                <td>
                                    @item.Link
                                </td>
                                
                                <td>


                                   
                                    <a asp-action="Update" asp-route-id="@item.Id" class="effect effect-2 update">Duzelish et</a>

                                </td>
                            </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


