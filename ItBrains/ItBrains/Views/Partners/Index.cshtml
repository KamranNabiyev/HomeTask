
@{
    ViewData["Title"] = "Index";
}

<!-- ***** Partners Start ***** -->
<section class="section" id="partners">
    <div class="container">
        <!-- ***** Section Title Start ***** -->
        <div class="row">
            <div class="col-lg-12">
                <div class="section-heading">
                    <h2>Partnyorlar</h2>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="owl-partners owl-carousel">
                <div clas="item " style="width: 100%; height: 300px; display: flex; align-items: center">
                    <img style="width:100%;height:160px;object-fit:cover" src="~/src/img/logo/bgm.png" alt="Baki gencler merkezi" />
                </div>
                <div clas="item " style="width:100%;height:300px;display:flex;align-items:center">
                    <img style="width:100%;height:120px;object-fit:cover" src="~/src/img/logo/fes.png" alt="Baki gencler merkezi" />
                </div>
                <div clas="item " style="width: 100%; height: 300px; display: flex; align-items: center ">
                    <img style="width:100%;height:150px;object-fit:cover" src="~/src/img/logo/Dövlət_Məşğulluq_Agentliyi_logo.png" alt="Baki gencler merkezi" />
                </div>
                <div clas="item " style="width: 100%; height: 300px; display: flex; align-items: center ">
                    <img style="width:100%;height:300px;object-fit:cover" src="~/src/img/logo/metabat2.png" alt="Baki gencler merkezi" />
                </div>
                <div clas="item " style="width: 100%; height: 300px; display: flex; align-items: center ">
                    <img style="width:100%;height:300px;object-fit:cover" src="~/src/img/logo/oscorp.jpg" alt="Baki gencler merkezi" />
                </div>
            </div>
        </div>

        <!-- ***** Section Title End ***** -->
    </div>
</section>
<!-- ***** Partners End ***** -->
@section Css{
    <link rel="stylesheet"asp-append-version="true"  href="~/src/css/secondary.css" />
}